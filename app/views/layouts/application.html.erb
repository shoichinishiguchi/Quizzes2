<!DOCTYPE html>
<html>
  <head>
    <title>Quizzes</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.100.2/css/materialize.min.css" >
    <script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.100.2/js/materialize.min.js" ></script>

  </head>

  <body>
    <div class="row">
      <div class="col s8 teal lighten-1 header">
        <a class="header" href="/quizzes"> Quizzes! </a>
      </div>
      <div class="col s4 cyan lighten-1  header">
        <a class="waves-effect waves-light btn" href="/quizzes/new"> <i class="material-icons left"></i>出題する</a>
      </div>
    </div>

    <%= yield %>
  </body>
</html>
